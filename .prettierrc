{
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.)$)|^(react-native(.)$)",
    "^(expo(.*)$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^~/components/(.*)$",
    "^~/utils/(.*)$",
    "^~/(.*)$",
    "^[./]"
  ]
}
